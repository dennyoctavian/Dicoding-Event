<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.dennyoctavian.dicodingevent.fragments.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.dennyoctavian.dicodingevent.fragments.UpcomingFragment"
        android:label="Upcoming"
        tools:layout="@layout/fragment_upcoming" />
    <fragment
        android:id="@+id/navigation_finished"
        android:name="com.dennyoctavian.dicodingevent.fragments.FinishedFragment"
        android:label="Finished"
        tools:layout="@layout/fragment_finished" />
    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.dennyoctavian.dicodingevent.fragments.FavoriteFragment"
        android:label="Favorite"
        tools:layout="@layout/fragment_favorite" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.dennyoctavian.dicodingevent.fragments.SettingFragment"
